{
    "controllers": {
        "@spomky-labs/pwa-bundle": {
            "connection-status": {
                "enabled": true,
                "fetch": "eager"
            },
            "backgroundsync-form": {
                "enabled": true,
                "fetch": "eager"
            },
            "sync-broadcast": {
                "enabled": true,
                "fetch": "eager"
            },
            "prefetch-on-demand": {
                "enabled": true,
                "fetch": "eager"
            }
        },
        "@survos/api-grid-bundle": {
            "js_twig": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": []
            },
            "api_grid": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "datatables.net-bs5/css/dataTables.bootstrap5.min.css": true,
                    "datatables.net-buttons-bs5/css/buttons.bootstrap5.min.css": true,
                    "datatables.net-responsive-bs5/css/responsive.bootstrap5.min.css": true,
                    "datatables.net-scroller-bs5/css/scroller.bootstrap5.min.css": true,
                    "datatables.net-searchpanes-bs5/css/searchPanes.bootstrap5.min.css": true,
                    "datatables.net-searchbuilder-bs5/css/searchBuilder.bootstrap5.min.css": true,
                    "datatables.net-select-bs5/css/select.bootstrap5.min.css": true
                }
            },
            "grid": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "datatables.net-bs5/css/dataTables.bootstrap5.min.css": true,
                    "datatables.net-buttons-bs5/css/buttons.bootstrap5.min.css": true,
                    "datatables.net-responsive-bs5/css/responsive.bootstrap5.min.css": true,
                    "datatables.net-scroller-bs5/css/scroller.bootstrap5.min.css": true,
                    "datatables.net-searchpanes-bs5/css/searchPanes.bootstrap5.min.css": true,
                    "datatables.net-searchbuilder-bs5/css/searchBuilder.bootstrap5.min.css": true,
                    "datatables.net-select-bs5/css/select.bootstrap5.min.css": true
                }
            }
        },
        "@survos/bootstrap-bundle": {
            "search": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": []
            }
        },
        "@survos/pixie-bundle": {
            "pixie_apex": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "@survos/pixie/style.css": true
                }
            }
        },
        "@survos/simple-datatables-bundle": {
            "table": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "@survos/simple-datatables/style.css": true
                }
            }
        },
        "@survos/tree-bundle": {
            "tree": {
                "enabled": true,
                "fetch": "lazy"
            },
            "api_tree": {
                "enabled": true,
                "fetch": "lazy"
            }
        },
        "@survos/workflow-helper-bundle": {
            "workflow": {
                "enabled": true,
                "fetch": "eager"
            }
        },
        "@symfony/ux-autocomplete": {
            "autocomplete": {
                "enabled": true,
                "fetch": "eager",
                "autoimport": {
                    "tom-select/dist/css/tom-select.default.css": true,
                    "tom-select/dist/css/tom-select.bootstrap4.css": false,
                    "tom-select/dist/css/tom-select.bootstrap5.css": false
                }
            }
        },
        "@symfony/ux-chartjs": {
            "chart": {
                "enabled": true,
                "fetch": "eager"
            }
        },
        "@symfony/ux-dropzone": {
            "dropzone": {
                "enabled": true,
                "fetch": "eager",
                "autoimport": {
                    "@symfony/ux-dropzone/dist/style.min.css": true
                }
            }
        }
    },
    "entrypoints": []
}
