{
    "type": "project",
    "name": "survos-sites/translation-server",
    "description": "Async translation server",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "ext-redis": "*",
        "ext-sodium": "*",
        "ext-sqlite3": "*",
        "ext-xsl": "*",
        "ext-zip": "*",
        "api-platform/doctrine-orm": "^4.1.20",
        "api-platform/symfony": "^4.1.20",
        "doctrine/dbal": "^4.3.2",
        "doctrine/doctrine-bundle": "^2.15.1",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.5.2",
        "easycorp/easyadmin-bundle": "^4.24.9",
        "halaxa/json-machine": "^1.2.5",
        "inspector-apm/inspector-symfony": ">=1.8.2",
        "nelmio/cors-bundle": "^2.5",
        "nyholm/psr7": "^1.8.2",
        "paragonie/sodium_compat": "^2.1",
        "php-http/guzzle7-adapter": "^1.1",
        "phpdocumentor/reflection-docblock": "^5.6.2",
        "phpstan/phpdoc-parser": "^2.2",
        "psr/log": "^3.0.2",
        "survos/bootstrap-bundle": "^1.6.44",
        "survos/bunny-bundle": "^1.6.44",
        "survos/command-bundle": "^1.6.44",
        "survos/core-bundle": ">=1.6.44",
        "survos/deployment-bundle": "^1.6.44",
        "survos/libre-translate-bundle": "^1.6.44",
        "survos/simple-datatables-bundle": "^1.6.44",
        "survos/workflow-helper-bundle": ">=1.6.44",
        "symfony/amqp-messenger": "^7.3.2",
        "symfony/asset": "^7.3",
        "symfony/asset-mapper": "^7.3.2",
        "symfony/console": "^7.3.2",
        "symfony/debug-bundle": "^7.3",
        "symfony/doctrine-messenger": "^7.3.2",
        "symfony/dotenv": "^7.3.2",
        "symfony/expression-language": "^7.3.2",
        "symfony/flex": "^2.8.1",
        "symfony/framework-bundle": "^7.3.2",
        "symfony/http-client": "^7.3.2",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "^7.3.2",
        "symfony/property-info": "^7.3.1",
        "symfony/runtime": "^7.3.1",
        "symfony/security-bundle": "^7.3.2",
        "symfony/serializer": "^7.3.2",
        "symfony/string": "^7.3.2",
        "symfony/translation": "^7.3.2",
        "symfony/translation-contracts": ">=3.6.0",
        "symfony/twig-bundle": "^7.3.2",
        "symfony/ux-chartjs": "^2.29.1",
        "symfony/validator": "^7.3.2",
        "symfony/yaml": "^7.3.2",
        "twig/extra-bundle": "^3.21",
        "twig/string-extra": "^3.21",
        "vanderlee/php-sentence": "^1.0.8",
        "zenstruck/bytes": ">=1"
    },
    "config": {
        "allow-plugins": {
            "endroid/installer": true,
            "php-http/discovery": true,
            "survos/installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "export": [
            "php -d memory_limit=2G bin/console app:export --zip --no-debug --batch=2500"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "diagram": "bin/console doctrine:diagram --filename=assets/db.svg --exclude=doctrine_migration_versions,messenger_messages"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.3"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "jawira/doctrine-diagram-bundle": "^0.8.0",
        "phpstan/phpstan": "^2.1.22",
        "phpunit/phpunit": "^12.3.4",
        "survos/maker-bundle": "^1.6.44",
        "symfony/maker-bundle": "^1.64.0",
        "symfony/web-profiler-bundle": "^7.3.2"
    }
}
